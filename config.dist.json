{
    "xmdb_filename_template": "Good{}.xmdb",
    "path_xmdb": "./xmdb",
    "cmd_decompress": "7z e -o{destination_folder} {source_file}",
    "cmd_compress": "7z a -t7z -mx=9 -ms=on -md=128m -mmt=on {destination_file}",
    "compressed_extension": "7z",
    "exclude_file_regex": "(\\[v\\]|\\[eb[ab]\\]|Emulation|Audio Players|E-Reader Cards|Slideshow|ReadBoy|PictureBoy|GoodBook|Pocketnes|PCEAdvance|Goomba|SMSAdvance|\\d+[- ](Games )?in[- ]1)",
    "log_level": 30
}
